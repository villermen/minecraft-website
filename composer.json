{
    "name": "villermen/minecraft-website",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Villermen",
            "email": "villermen@gmail.com"
        }
    ],
    "scripts": {
        "dev-server": [
            "Composer\\Config::disableProcessTimeout",
            "php -S localhost:8080 -t public public/index.php"
        ],
        "cache:clear": "rm -f cache/*",
        "post-install-cmd": "@cache:clear"
    },
    "autoload": {
        "psr-4": {
            "Villermen\\Minecraft\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "sort-packages": true
    },
    "require": {
        "php": ">=8.2",
        "ext-gd": "*",
        "ext-json": "*",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-foundation": "^7.3",
        "symfony/yaml": "^7.3",
        "twig/twig": "^3.22",
        "xpaw/php-minecraft-query": "^5.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.3"
    }
}
